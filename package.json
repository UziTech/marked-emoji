{
  "name": "marked-emoji",
  "version": "2.0.1",
  "description": "Add text emoji support to marked",
  "main": "./lib/index.cjs",
  "module": "./src/index.js",
  "browser": "./lib/index.umd.js",
  "types": "./src/index.d.ts",
  "type": "module",
  "keywords": [
    "marked",
    "extension",
    "emoji",
    "emojis"
  ],
  "files": [
    "lib/",
    "src/"
  ],
  "exports": {
    ".": {
      "import": "./src/index.js",
      "require": "./lib/index.cjs"
    }
  },
  "scripts": {
    "test": "node --experimental-vm-modules ./node_modules/jest-cli/bin/jest.js --verbose",
    "test:cover": "node --experimental-vm-modules ./node_modules/jest-cli/bin/jest.js --coverage",
    "test:types": "tsd -t src/index.d.ts -f types_test/index.test-d.ts",
    "lint": "eslint",
    "build": "rollup -c rollup.config.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/UziTech/marked-emoji.git"
  },
  "author": "Tony Brix <Tony@Brix.ninja> (https://Tony.Brix.ninja)",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/UziTech/marked-emoji/issues"
  },
  "homepage": "https://github.com/UziTech/marked-emoji#readme",
  "peerDependencies": {
    "marked": ">=4 <17"
  },
  "devDependencies": {
    "@markedjs/eslint-config": "^1.0.12",
    "@octokit/plugin-throttling": "^11.0.1",
    "@octokit/rest": "^22.0.0",
    "@semantic-release/changelog": "^6.0.3",
    "@semantic-release/commit-analyzer": "^13.0.1",
    "@semantic-release/git": "^10.0.1",
    "@semantic-release/github": "^11.0.3",
    "@semantic-release/npm": "^12.0.1",
    "@semantic-release/release-notes-generator": "^14.0.3",
    "@types/marked": "^6.0.0",
    "eslint": "^9.29.0",
    "globals": "^16.2.0",
    "jest-cli": "^30.0.2",
    "marked": "^16.0.0",
    "rollup": "^4.44.0",
    "semantic-release": "^24.2.5",
    "tsd": "^0.32.0"
  },
  "tsd": {
    "compilerOptions": {
      "paths": {
        "marked-emoji": [
          "./src"
        ]
      }
    }
  }
}
